{
  "hosting": {
    "public": "output",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
	"cleanUrls": true,
	"rewrites": [ {
		"source": "**",
		"destination": "/index.html"
	} ],
    "headers": [{
      "source" : "**",
      "headers" : [{
        "key" : "X-Powered-By",
        "value" : "corlaez"
      },{
        "key" : "X-Frame-Options",
        "value" : "SAMEORIGIN"
      },{
        "key" : "X-Content-Type-Options",
        "value" : "nosniff"
      },{
        "key" : "Referrer-Policy",
        "value" : "same-origin"
      }  ]
    } ]
  }
}
